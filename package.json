{
  "name": "vite-vue3-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "cross-env NODE_ENV=development vite --mode test",
    "build:dev": "cross-env NODE_ENV=development vue-tsc && vite build --mode development",
    "build:test": "cross-env NODE_ENV=development vue-tsc && vite build",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^10.2.1",
    "axios": "^1.6.2",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.4.2",
    "less": "^4.1.3",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "mock2js": "^1.0.6",
    "prettier": "2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
